{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "local",
        "address": "223.6.6.6",
        "detour": "direct-out"
      },
      {
        "tag": "dns_DOH",
        "address": "https://doh.360.cn/dns-query",
        "address_resolver": "local"
      }
    ],
    "rules": [
      {
        "action": "route",
        "outbound": "any",
        "server": "local"
      }
    ],
    "final": "dns_DOH",
    "strategy": "prefer_ipv4", // 删掉IPv6测试
    "reverse_mapping": true,
    "independent_cache": false
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "stack": "system", // 游戏测试
      "auto_route": true,
      "auto_redirect": false,
      "strict_route": true
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct-out"
    },
    {
      "type": "vmess",
      "tag": "home-proxy",
      "server": "服务器地址",
      "server_port": 1080,
      "uuid": "填入uuid",
      "security": "auto",
      "alter_id": 0,
      "packet_encoding": "xudp", // 测试 packetaddr 游戏延迟
      "transport": {
        "type": "ws",
        "headers": {
          "Host": "混淆地址"
        }
      }
    },
    {
      "type": "selector",
      "tag": "select",
      "outbounds": [
        "home-proxy"
      ],
      "default": "home-proxy",
      "interrupt_exist_connections": true
    }
  ],
  "route": {
    "rules": [
      {
        "sniffer": [
          "dns"
        ],
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      }
    ],
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "final": "select"
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_rdrc": true
    },
    "clash_api": {
      "secret": "123456",
      "external_ui": "xd",
      "external_controller": "0.0.0.0:9090",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "select"
    }
  }
}